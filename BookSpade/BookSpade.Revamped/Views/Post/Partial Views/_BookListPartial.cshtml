@model IEnumerable<BookSpade.Revamped.Models.Textbook>

@{
    foreach (var b in Model)
    { 
        <li>
          <div class="item">
            <span id="Course" class="Block">@b.Author</span>
            <span id="Title" class="Block Truncate One-line" data-value="@b.TextBookId" >@b.BookTitle</span>
          </div>
        </li>
    }
}